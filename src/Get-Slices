# NorNet
from NorNetSiteSetup import *
from NorNetTools     import *
from NorNetAPI       import *



# ###### Main program #######################################################
loginToPLC(quietMode = True)

sliceList = fetchNorNetSlice(None)
for slice in sliceList:
   # print(slice)
   sliceName  = slice['slice_name']

   print(sliceName)
